   "stack": "Rabbit#1895",
   "buildpacks": [{
      "url": ""
   }],
   "env": {
      "TOKEN": {
         "description": "The bot token (Remember! The TOKEN is super secret)",
         "required": true
      },
      "PREFIX": {
         "description": "The default bot prefix (eg. r!)",
         "required": true
      },
      "AMEAPI": {
         "description": "Your Amethyste api token",
         "required": true
      },
      "COOKIES": {
         "description": "Your Youtube cookies",
         "required": false
      },
      "BRAINID": {
         "description": "Your Brainshop AI Brain ID",
         "required": true
      },
      "BRAINKEY": {
         "description": "Your Brainshop AI Brain Key",
         "required": true
      },
      
      "ID": {
         "description": "Your Discord Bot ID",
         "required": true
      },
      "STATUS_WEBHOOK_TOKEN": {
         "description": "Your status webhook token",
         "required": false
