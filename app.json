      },
      "GENIUS": {
         "description": "Your Genius API key",
         "required": true
      },
      "MYSQL_DATABASE": {
         "description": "Your MYSQL Database name",
         "required": true
      },
      "MYSQL_HOST": {
         "description": "Your MYSQL Host Name",
